<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.ale_j.xyz_4.casa"
    android:background="@drawable/fondo3"
    >
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="AÑADIR INMUEBLE"
        android:textSize="20dp"
        android:textAlignment="center"
        android:textColor="#ffff"
        android:id="@+id/textView2" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView2"
        android:layout_marginStart="34dp"
        android:layout_marginTop="29dp"
        android:background="#bcffffff"
        android:text="Galeria"
        android:textColor="#000f41"
        android:id="@+id/button" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/button"
        android:layout_alignBottom="@+id/button"
        android:layout_toEndOf="@+id/hab"
        android:background="#bcffffff"
        android:text="Camara"
        android:textColor="#000f41" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/button"
        android:layout_marginStart="15dp"
        android:layout_marginTop="28dp"
        android:text="NUM DE HAB:"
        android:textColor="#e6f1fa"
        android:textStyle="bold" />

    <EditText
        android:id="@+id/hab"
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView3"
        android:layout_alignBottom="@+id/textView3"
        android:layout_marginStart="22dp"
        android:layout_toEndOf="@+id/textView6"
        android:background="#ffff"
        android:textColor="#001841" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView3"

        android:layout_below="@+id/hab"
        android:layout_marginTop="27dp"
        android:text="NUM DE BAÑOS:"
        android:textColor="#e6f1fa"
        android:textStyle="bold"
        android:id="@+id/textView4" />

    <EditText
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView4"
        android:layout_alignStart="@+id/hab"
        android:background="#ffff"
        android:textColor="#001841"
        android:id="@+id/editText" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView4"
        android:layout_below="@+id/textView4"
        android:layout_marginTop="20dp"
        android:text="SUPERFICIE"
        android:textColor="#e6f1fa"
        android:textStyle="bold" />

    <EditText
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView5"
        android:layout_alignStart="@+id/editText"
        android:background="#ffff"
        android:textColor="#001841"
        android:id="@+id/sup"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/sup"
        android:layout_alignStart="@+id/button2"
        android:layout_marginStart="22dp"
        android:text="m2"
        android:textColor="#e6f1fa"
        android:textStyle="bold"
        android:id="@+id/textView9" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView5"
        android:layout_below="@+id/textView5"
        android:layout_marginTop="16dp"
        android:text="AÑO DE CONSTRUCCION"
        android:textColor="#e6f1fa"
        android:textStyle="bold"
        android:id="@+id/textView6" />

    <EditText
        android:id="@+id/aconst"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView6"
        android:layout_alignBottom="@+id/textView6"
        android:layout_alignStart="@+id/sup"
        android:background="#ffff"
        android:textColor="#001841" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"

        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView6"
        android:layout_below="@+id/aconst"
        android:layout_marginTop="23dp"
        android:text="TIPO"
        android:textColor="#e6f1fa"
        android:textStyle="bold" />

    <EditText
        android:id="@+id/tipo"
        android:layout_width="90dp"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView7"
        android:layout_alignBottom="@+id/textView7"
        android:layout_alignStart="@+id/aconst"
        android:background="#ffff" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView7"
        android:layout_below="@+id/tipo"
        android:layout_marginTop="22dp"
        android:text="DUEÑO"
        android:textColor="#e6f1fa"
        android:textStyle="bold" />

    <EditText
        android:id="@+id/duen"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/tipo"
        android:layout_alignTop="@+id/textView8"
        android:background="#ffff" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView8"
        android:layout_below="@+id/duen"
        android:layout_marginTop="18dp"
        android:text="SEXO"
        android:textColor="#e6f1fa"
        android:textStyle="bold"
        android:id="@+id/textView10" />

    <EditText
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView10"

        android:layout_alignStart="@+id/duen"
        android:background="#ffff" />

    <Button
        android:id="@+id/mapas"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="37dp"
        android:layout_toEndOf="@+id/textView7"
        android:background="#bcffffff"
        android:text="MAPAS"
        android:textColor="#000f41" />
    <Button
        android:id="@+id/enviar"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_alignEnd="@+id/duen"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="36dp"
        android:background="#bcffffff"
        android:text="ENVIAR"
        android:textColor="#000f41" />







</RelativeLayout>
