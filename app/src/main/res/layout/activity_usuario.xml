<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.ale_j.xyz_4.usuario">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="17dp"
        android:text="Menu Usuario"
        android:textAlignment="center"
        android:textColor="#011633"
        android:textSize="22dp"
        android:textStyle="bold"
        android:id="@+id/textView2" />
    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/reg"
        android:background="#fffdf1"
        android:src="@drawable/b1"
        android:layout_marginStart="38dp"
        android:layout_marginTop="55dp"
        android:layout_below="@+id/textView2"
        android:layout_alignParentStart="true" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="REGISTRARSE"
        android:textColor="#011633"
        android:layout_marginTop="14dp"
        android:layout_below="@+id/reg"
        android:layout_alignStart="@+id/reg"
        android:id="@+id/textView5" />
    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/oft"
        android:src="@drawable/b2"
        android:background="#fffdf1"
        android:layout_alignTop="@+id/reg"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="54dp" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="VER OFERTAS"
        android:textColor="#011633"
        android:layout_alignBottom="@+id/textView5"
        android:layout_alignStart="@+id/oft"
        android:layout_marginStart="11dp"
        android:layout_alignTop="@+id/textView5"
        android:id="@+id/textView7" />
    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/busc"
        android:src="@drawable/b3"
        android:background="#fffdf1"
        android:layout_below="@+id/textView5"
        android:layout_alignStart="@+id/textView5"
        android:layout_marginTop="65dp" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="BUSCAR"
        android:textColor="#011633"
        android:layout_below="@+id/busc"
        android:layout_alignStart="@+id/busc"
        android:layout_marginStart="16dp"
        android:layout_marginTop="22dp"
        android:id="@+id/textView6" />
    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/cont"
        android:src="@drawable/boton4"
        android:background="#fffdf1"
        android:layout_alignTop="@+id/busc"
        android:layout_alignStart="@+id/oft" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CONTACTAR"
        android:textColor="#011633"
        android:layout_alignBaseline="@+id/textView6"
        android:layout_alignBottom="@+id/textView6"
        android:layout_alignStart="@+id/textView7" />

</RelativeLayout>
